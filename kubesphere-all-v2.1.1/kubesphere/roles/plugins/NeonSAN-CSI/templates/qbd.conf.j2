[zookeeper]
# IP of zookeeper cluster
ip="{{ neonsan_server_address }}"
cluster_name="{{ neonsan_cluster_name }}"

[client]
tcp_no_delay={{ client_tcp_no_delay }}
io_depth={{ client_io_depth }}
io_timeout={{ client_io_timeout }}
conn_timeout={{ conn_timeout }}
open_volume_timeout={{ open_volume_timeout }}